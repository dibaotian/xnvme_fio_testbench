# IOPS
# queue depth = 32
# workers = 8	
# block size       4K       8K	 
# random write	   170000	85000
# random read	   700000	625000
# 70read 30write   290000	175000

[global]
direct=1
ioengine=libaio
randrepeat=0
bs=4k
iodepth=32
size=100%
group_reporting=1
time_based
runtime=5

[job1-4k-rand write]
rw=randread
numjobs=8
filename=/dev/nvme0n1
stonewall