# Latency
# queue depth = 1
# workers = 1	
# block size  4K
# iodepth  1
# seq   read     write
#       10us     13us   
# rand  read     write
#       82us     15us     	 

[global]
direct=1
ioengine=libaio
randrepeat=0
bs=4k
iodepth=1
size=100%
#group_reporting=1
time_based
runtime=5

[job1 seq read]
rw=read
numjobs=1
filename=/dev/nvme0n1
stonewall

[job2 seq write]
rw=write
numjobs=1
filename=/dev/nvme0n1
stonewall

[job3 rand write]
rw=randread
numjobs=1
filename=/dev/nvme0n1
stonewall

[job4 rand write]
rw=randwrite
numjobs=1
filename=/dev/nvme0n1
stonewall